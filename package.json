{
  "name": "@alpic-ai/mcp-eval",
  "description": "A CLI to evaluate MCP servers performance",
  "version": "0.1.2",
  "author": "Frédéric Barthelet",
  "bin": {
    "@alpic-ai/mcp-eval": "./bin/run.js"
  },
  "bugs": "https://github.com/alpic-ai/mcp-eval/issues",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.5",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "openai": "^5.19.1",
    "yaml": "^2.8.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^22.18.1",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/alpic-ai/mcp-eval",
  "keywords": [
    "mcp",
    "model context protocol",
    "performance",
    "evaluation",
    "test"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "mcp-eval",
    "dirname": "mcp-eval",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "alpic-ai/mcp-eval",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "vitest",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
